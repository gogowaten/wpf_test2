<Window x:Class="_20191230_pdf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20191230_pdf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
    <StackPanel>
      <Button Name="ButtonOpen" Content="open" Click="ButtonOpen_Click"/>
      <Button Name="ButtonOpenScale" Content="openScale" Click="ButtonOpenScale_Click"/>
      <Image x:Name="ImagePDF" Stretch="None"/>
    </StackPanel>
    </Grid>
</Window>
